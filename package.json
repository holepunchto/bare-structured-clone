{
  "name": "bare-structured-clone",
  "version": "1.4.1",
  "description": "Structured cloning algorithm for JavaScript",
  "exports": {
    "./package": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./global": {
      "types": "./global.d.ts",
      "default": "./global.js"
    },
    "./constants": {
      "types": "./lib/constants.d.ts",
      "default": "./lib/constants.js"
    },
    "./errors": {
      "types": "./lib/errors.d.ts",
      "default": "./lib/errors.js"
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "global.js",
    "global.d.ts",
    "binding.c",
    "binding.js",
    "CMakeLists.txt",
    "lib",
    "prebuilds"
  ],
  "addon": true,
  "scripts": {
    "test": "prettier . --check && bare test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-structured-clone.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-structured-clone/issues"
  },
  "homepage": "https://github.com/holepunchto/bare-structured-clone#readme",
  "engines": {
    "bare": ">=1.2.0"
  },
  "dependencies": {
    "bare-type": "^1.0.0",
    "compact-encoding": "^2.15.0"
  },
  "devDependencies": {
    "bare-buffer": "^3.0.1",
    "bare-url": "^2.1.1",
    "brittle": "^3.1.1",
    "cmake-bare": "^1.1.6",
    "prettier": "^3.3.3",
    "prettier-config-standard": "^7.0.0"
  },
  "peerDependencies": {
    "bare-buffer": "*",
    "bare-url": "*"
  },
  "peerDependenciesMeta": {
    "bare-buffer": {
      "optional": true
    },
    "bare-url": {
      "optional": true
    }
  }
}
